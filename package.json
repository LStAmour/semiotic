{
  "name": "semiotic",
  "version": "1.16.3",
  "description": "The semiotic JavaScript data visualization framework",
  "main": "lib/index.js",
  "unpkg": "dist/semiotic.min.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "BABEL_ENV=test NODE_ENV=test PUBLIC_URL='' jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:old": "node scripts/test.js --env=jsdom",
    "pretest:dist": "npm run dist && rm -rf screenshots && mkdir screenshots",
    "test:dist": "node scripts/test-dist.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "flow": "flow",
    "prebuild": "babel src/components --out-dir lib --ignore test.js",
    "create-release-branch": "sh ./scripts/create-release-branch.sh",
    "publish-release": "sh ./scripts/publish-release.sh",
    "deploy-docs": "sh ./scripts/docs.sh",
    "ci-deploy-docs": "sh ./scripts/docs-ci.sh",
    "gh-pages": "REACT_APP_GH_PAGES_PATH='semiotic' npm run build",
    "dist": "rollup -c && uglifyjs ./dist/semiotic.js -o ./dist/semiotic.min.js",
    "docs-bundle-stats": "GENERATE_SOURCEMAP='true' NODE_ENV='production' webpack --config ./config/webpack.config.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "prepublishOnly": "npm run dist && rm ./dist/semiotic.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emeeks/semiotic.git"
  },
  "author": {
    "name": "Elijah Meeks",
    "email": "elijahmeeks@gmail.com"
  },
  "contributors": [
    "Susie Lu <susie.lu.shan@gmail.com>",
    "James J. Womack (@james_womack)"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.2.2",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@vx/pattern": "0.0.183",
    "autoprefixer": "9.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.5",
    "babel-preset-netflix-dea": "^4.0.0",
    "babel-preset-react-app": "^7.0.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.2",
    "classnames": "2.2.6",
    "conventional-changelog-cli": "2.0.11",
    "conventional-recommended-bump": "4.0.4",
    "coveralls": "^3.0.1",
    "css-loader": "2.1.0",
    "d3-dsv": "1.0.10",
    "d3-random": "1.1.2",
    "d3-sankey": "0.7.1",
    "dagre": "^0.8.2",
    "dentist": "1.0.3",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-netflix-dea": "4.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.12.3",
    "file-loader": "3.0.1",
    "flow-bin": "^0.89.0",
    "fs-extra": "7.0.1",
    "history": "4.7.2",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "in-publish": "2.0.0",
    "jest": "23.6.0",
    "jsdom": "13.1.0",
    "less": "3.9.0",
    "material-design-icons-svg": "2.0.0",
    "material-ui": "v1.0.0-beta.13",
    "material-ui-icons": "1.0.0-beta.36",
    "mini-css-extract-plugin": "^0.5.0",
    "nyc": "13.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "puppeteer": "^1.5.0",
    "react": "16.7.0",
    "react-dev-utils": "^7.0.1",
    "react-dimensions": "git://github.com/AlesJiranek/react-dimensions",
    "react-dom": "16.7.0",
    "react-element-to-jsx-string": "^14.0.2",
    "react-prism": "4.3.2",
    "react-router": "^4.0.0-beta.6",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.7.0",
    "reactstrap": "git://github.com/jameswomack/reactstrap#react-popper_react-dom-16",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-regenerator": "^0.6.0",
    "rollup-plugin-replace": "^2.0.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "tap-difflet": "0.7.1",
    "tap-notify": "1.0.0",
    "tape": "4.9.2",
    "uglify-js": "^3.1.10",
    "url-loader": "1.1.2",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@mapbox/polylabel": "1",
    "d3-array": "^2.0.3",
    "d3-bboxCollide": "^1.0.3",
    "d3-brush": "^1.0.4",
    "d3-chord": "^1.0.4",
    "d3-collection": "^1.0.1",
    "d3-contour": "^1.1.1",
    "d3-force": "^2.0.0",
    "d3-glyphedge": "^1.2.0",
    "d3-hexbin": "^0.2.2",
    "d3-hierarchy": "^1.1.3",
    "d3-interpolate": "^1.1.5",
    "d3-polygon": "^1.0.5",
    "d3-sankey-circular": "0.28.0",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-voronoi": "^1.0.2",
    "json2csv": "4.3.2",
    "labella": "1.1.4",
    "memoize-one": "5.0.0",
    "object-assign": "4.1.1",
    "polygon-offset": "0.3.1",
    "promise": "8.0.2",
    "prop-types": "15.6.2",
    "react-annotation": "2.1.5",
    "roughjs-es5": "0.1.0",
    "semiotic-mark": "0.3.1",
    "svg-path-bounding-box": "1.0.4"
  },
  "nyc": {
    "exclude": [
      "tests/unit/lib/*.js",
      "tests/unit/specs/**/*.js"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/docs/**"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
